      </parameter>-->
      <!--<parameter>
        <parameterName value="@timestamp"/>
        <dbType value="Double"/>
        <layout type="Log4netCustom2.Layout.PatternLayout">
          <conversionPattern value="%timestamp"/>
        </layout>
      </parameter>-->
      <!--<parameter>
        <parameterName value="@file"/>
        <dbType value="String"/>
        <size value="255"/>
        <layout type="Log4netCustom2.Layout.PatternLayout">
          <conversionPattern value="%file"/>
        </layout>
